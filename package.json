{
  "name": "wdii-note-cards",
  "version": "1.0.0",
  "description": "Collaborative note cards repository for Web Development II",
  "main": "index.js",
  "scripts": {
    "validate": "bash ./scripts/validate-card.sh",
    "validate:all": "bash ./scripts/validate-card.sh cards/*.md",
    "lint:md": "markdownlint *.md cards/*.md .github/*.md",
    "lint:md:fix": "markdownlint --fix *.md cards/*.md .github/*.md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web-development",
    "education",
    "collaborative",
    "note-cards",
    "css",
    "html",
    "accessibility"
  ],
  "author": "Cynthia Teeters",
  "license": "MIT",
  "devDependencies": {
    "chalk": "^4.1.2",
    "glob": "^8.0.0",
    "gray-matter": "^4.0.3",
    "markdownlint-cli": "^0.45.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0"
  }
}
